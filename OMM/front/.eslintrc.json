{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/prop-types": "off",
        "react/button-has-type": "off",
        "jsx-a11y/label-has-associated-control": [
            2,
            {
                "labelAttributes": ["htmlFor"]
            }
        ],
        "no-console": "off",
        "prefer-const": "off"
    }
}
